# Demo Configuration
DEMO_MODE=true
SIMULATION_ONLY=true

# Sui Blockchain Configuration
# For real transactions, set these values appropriately
SUI_NETWORK=testnet
SUI_RPC_URL=https://fullnode.testnet.sui.io:443

# Mock Blockchain Configuration (for demo mode)
MOCK_RPC_URL=https://mock-sui-node.example.com
MOCK_FAUCET_URL=https://mock-sui-faucet.example.com

# Wallet Configuration (DO NOT USE REAL SEEDS/KEYS HERE)
